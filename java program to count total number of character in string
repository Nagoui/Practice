// java program to count total number of character in string
public class Main
{
	public static void main(String[] args) {
		String  string = "The best of both worlds";
		int count = 0;
		
		for(int i=0;i<string.length();i++){
		    if(string.charAt(i) != ' ')    {
		   // if(string.charAt(i)!='')    
		    count++;
		    }
		}
		System.out.println("totoal number of char "+count);
		
	}
}
